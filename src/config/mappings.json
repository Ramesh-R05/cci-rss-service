{
  "channel": {
    "default": {
      "title": {
        "map": [ "pageTitle_t", "contentTitle_t" ]
      },
      "feed_url": {
        "map": {
          "fn": "mapFeedUrl",
          "params": [ "@__request" ]
        }
      },
      "site_url": {
        "map": "siteUrl_t"
      },
      "description": {
        "map": [ "pageMetaDescription_t", "contentSummary_t" ],
        "afterMap": [
          {
            "fn": "sanitise"
          }
        ]
      },
      "copyright": {
        "map": {
          "fn": "mapCopyright"
        }
      },
      "ttl": {
        "value": 60
      },
      "custom_namespaces": {
        "mapObject": {
          "media": {
            "value": "http://search.yahoo.com/mrss/"
          },
          "mi": {
            "value": "http://schemas.ingestion.microsoft.com/common/"
          }
        }
      }
    },
    "sponsored": {
      "custom_namespaces": {
        "mapObject": {
          "mvcf": {
            "value": "http://feed.aww.com.au/ns/mvcf/"
          }
        }
      }

    }
  },

  "item": {
    "default": {
      "title": {
        "map": [ "contentSummaryTitle_t", "contentTitle_t", "nodeName_t" ]
      },
      "description": {
        "map": [ "pageMetaDescription_t", "contentSummary_t" ],
        "afterMap": [
          {
            "fn": "sanitise"
          }
        ]
      },
      "url": {
        "map": {
          "fn": "mapItemUrl",
          "params": [ "@siteUrl_t", "@url_t" ]
        }
      },

      "enclosure": {
        "mapObject": {
          "url": {
            "map": "contentImageUrl_t",
            "afterMap": [
              {
                "fn": "format",
                "params": [ "%s?height=600" ]
              }
            ]
          },
          "type": {
            "map": {
              "fn": "mapMimeType",
              "params": [ "@contentImageUrl_t" ]
            }
          }
        }
      },
      "custom_elements": {
        "mapObjectArray": {
          "media:content": {
            "mapObjectArray": {
              "_attr":{
                "mapObject": {
                  "url": {
                    "map": "contentImageUrl_t",
                    "afterMap": [
                      {
                        "fn": "format",
                        "params": [ "%s?height=600" ]
                      }
                    ]
                  },
                  "type": {
                    "map": {
                      "fn": "mapMimeType",
                      "params": [ "@contentImageUrl_t" ]
                    }
                  }
                }
              },
              "media:thumbnail": {
                "mapObjectArray": {
                  "_attr":{
                    "mapObject": {
                      "url": {
                        "map": "contentImageUrl_t",
                        "afterMap": [
                          {
                            "fn": "format",
                            "params": [ "%s?height=600" ]
                          }
                        ]
                      },
                      "type": {
                        "map": {
                          "fn": "mapMimeType",
                          "params": [ "@contentImageUrl_t" ]
                        }
                      }
                    }
                  }
                }
              },
              "media:title": {
                "map": [ "contentImageAltText_t", "contentTitle_t", "nodeName_t" ]
              },
              "media:text": {
                "map": [ "contentImageCaption_t", "pageMetaDescription_t", "contentSummary_t" ]
              },
              "media:credit": {
                "map": {
                  "fn": "mapMediaCredit",
                  "params": [ "@contentPhotoCredit_t", "Provided by Bauer Media Pty Ltd" ]
                }
              },
              "mi:dateTimeWritten": {
                "map": [ "pageDateCreated_dt" ]
              },
              "mi:hasSyndicationRights": {
                "value": "1"
              },
              "mi:licenseId": {
                "value": "698525"
              },
              "mi:licensorName": {
                "value": "BAUER MEDIA PTY LIMITED"
              }
            }
          }
        }
      },
      "date": {
        "map": [ "pageDateCreated_dt" ]
      },
      "updatedDate": {
        "map": [ "nodeDateIndexed_dt" ]
      },
      "author": {
        "map": {
          "fn": "mapAuthorContent",
          "params": [ "@contentProfiles_t", "@siteName_t" ]
        }
      }
    },

    "fullcontent": {
      "custom_elements": {
        "mapObjectArray": {
          "content:encoded": {
            "mapObject": {
              "_cdata": {
                "map": {
                  "fn": "mapFullContent",
                  "params": [
                    {
                      "fn": "mapItemUrl",
                      "params": [
                        "@siteUrl_t",
                        "@url_t"
                      ]
                    },
                    "@contentBody_t"
                  ]
                }
              }
            }
          },
          "mi:dateTimeWritten": {
            "map": [ "pageDateCreated_dt" ]
          },
          "mi:hasSyndicationRights": {
            "value": "1"
          },
          "mi:licensorName": {
            "value": "BAUER MEDIA PTY LIMITED"
          },
          "media:content": {
            "mapObjectArray": {
              "_attr":{
                "mapObject": {
                  "url": {
                    "map": "contentImageUrl_t",
                    "afterMap": [
                      {
                        "fn": "format",
                        "params": [ "%s?height=600" ]
                      }
                    ]
                  },
                  "type": {
                    "map": {
                      "fn": "mapMimeType",
                      "params": [ "@contentImageUrl_t" ]
                    }
                  }
                }
              },
              "media:thumbnail": {
                "mapObjectArray": {
                  "_attr":{
                    "mapObject": {
                      "url": {
                        "map": "contentImageUrl_t",
                        "afterMap": [
                          {
                            "fn": "format",
                            "params": [ "%s?height=600" ]
                          }
                        ]
                      },
                      "type": {
                        "map": {
                          "fn": "mapMimeType",
                          "params": [ "@contentImageUrl_t" ]
                        }
                      }
                    }
                  }
                }
              },
              "media:title": {
                "map": [ "contentImageAltText_t", "contentTitle_t", "nodeName_t" ]
              },
              "media:text": {
                "map": [ "contentImageCaption_t", "pageMetaDescription_t", "contentSummary_t" ]
              },
              "media:credit": {
                "map": {
                  "fn": "mapMediaCredit",
                  "params": [ "@contentPhotoCredit_t", "Provided by Bauer Media Pty Ltd" ]
                }
              }
            }
          }
        }
      }
    },

    "fullcontentsourcefilter": {
      "custom_elements": {
        "mapObjectArray": {
          "content:encoded": {
            "mapObject": {
              "_cdata": {
                "map": {
                  "fn": "mapFullContent",
                  "params": [
                    {
                      "fn": "mapItemUrl",
                      "params": [
                        "@siteUrl_t",
                        "@url_t"
                      ]
                    },
                    "@contentBody_t"
                  ]
                }
              }
            }
          },
          "mi:dateTimeWritten": {
            "map": [ "pageDateCreated_dt" ]
          },
          "mi:hasSyndicationRights": {
            "value": "1"
          },
          "mi:licensorName": {
            "value": "BAUER MEDIA PTY LIMITED"
          },
          "media:content": {
            "mapObjectArray": {
              "_attr":{
                "mapObject": {
                  "url": {
                    "map": "contentImageUrl_t",
                    "afterMap": [
                      {
                        "fn": "format",
                        "params": [ "%s?height=600" ]
                      }
                    ]
                  },
                  "type": {
                    "map": {
                      "fn": "mapMimeType",
                      "params": [ "@contentImageUrl_t" ]
                    }
                  }
                }
              },
              "media:thumbnail": {
                "mapObjectArray": {
                  "_attr":{
                    "mapObject": {
                      "url": {
                        "map": "contentImageUrl_t",
                        "afterMap": [
                          {
                            "fn": "format",
                            "params": [ "%s?height=600" ]
                          }
                        ]
                      },
                      "type": {
                        "map": {
                          "fn": "mapMimeType",
                          "params": [ "@contentImageUrl_t" ]
                        }
                      }
                    }
                  }
                }
              },
              "media:title": {
                "map": [ "contentImageAltText_t", "contentTitle_t", "nodeName_t" ]
              },
              "media:text": {
                "map": [ "contentImageCaption_t", "pageMetaDescription_t", "contentSummary_t" ]
              },
              "media:credit": {
                "map": {
                  "fn": "mapMediaCredit",
                  "params": [ "@contentPhotoCredit_t", "Provided by Bauer Media Pty Ltd" ]
                }
              },
              "mi:dateTimeWritten": {
                "map": [ "pageDateCreated_dt" ]
              },
              "mi:hasSyndicationRights": {
                "value": "1"
              },
              "mi:licenseId": {
                "value": "698525"
              },
              "mi:licensorName": {
                "value": "BAUER MEDIA PTY LIMITED"
              }
            }
          }
        }
      }
    },

    "sponsored": {
      "categories": {
        "map": {
          "fn": "mapCategories",
          "params": [
            [
              "@__sections",
              {
                "fn": "mapCampaignSponsor",
                "params": [ "@contentCampaign_t" ]
              }
            ]
          ]
        }
      },
      "custom_elements": {
        "mapObjectArray": {
          "mvcf:is_bauer_native": {
            "map": {
              "fn": "mapCampaignType",
              "params": [ "@contentCampaign_t", "native" ]
            }
          },
          "mvcf:is_bauer_advertorial": {
            "map": {
              "fn": "mapCampaignType",
              "params": [ "@contentCampaign_t", "advertorial" ]
            }
          }
        }
      }
    },

    "sections": {
      "categories": {
        "map": "__sections"
      }
    },

    "recipes": {
      "categories": {
        "map": {
          "fn": "mapTags",
          "params": [ [ "@contentTags_ss", "@bauerTags_ss" ] ]
        }
      }
    },

    "recipes_fullcontent": {
      "custom_elements": {
        "mapObjectArray": {
          "content:encoded": {
            "mapObject": {
              "_cdata": {
                "map": {
                  "fn": "mapRecipeContent",
                  "params": [
                    [
                      {
                        "fn": "mapRecipeImage",
                        "params": [ "@contentImageUrl_t" ]
                      },
                      {
                        "fn": "mapRecipeServings",
                        "params": [ "@recipeServings_t" ]
                      },
                      {
                        "fn": "mapRecipeCookingTime",
                        "params": [ "@recipeCookingTime_t" ]
                      },
                      {
                        "fn": "mapRecipeIngredients",
                        "params": [ "@recipeIngredients_t" ]
                      },
                      {
                        "fn": "mapRecipeCookingMethod",
                        "params": [ "@recipeCookingMethod_t" ]
                      },
                      {
                        "fn": "mapRecipeTips",
                        "params": [ "@recipeTips_t" ]
                      },
                      {
                        "fn": "mapRecipeSource",
                        "params": [ "@recipeSource_s" ]
                      }
                    ]
                  ]
                }
              }
            }
          }
        }
      }
    }

  }

}
